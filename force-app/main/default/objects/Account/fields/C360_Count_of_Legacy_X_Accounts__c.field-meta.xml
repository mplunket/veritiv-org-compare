<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>C360_Count_of_Legacy_X_Accounts__c</fullName>
    <description>Returns 1 if the Legacy System Cd is &quot;X&quot; (case sensitive). Needed for Customer 360 calculation.</description>
    <externalId>false</externalId>
    <formula>IF(
and(
    ISBLANK( Legacy_System_Cd__c )=false ,
  Legacy_System_Cd__c =&quot;X&quot; )
,
1,0
)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Count of Legacy X Accounts</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <type>Number</type>
    <unique>false</unique>
</CustomField>

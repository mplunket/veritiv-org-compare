<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Canadian_A_Customer_Nbr__c</fullName>
    <description>This converts our Canadian customer number to a format that is used by other systems.  Only A customers come in to salesforce in account</description>
    <externalId>false</externalId>
    <formula>IF(  Legacy_System_Cd__c  = &quot;CA&quot;, (MID( Zpl_Customer_ID__c , 6, 15)),&quot;&quot;)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Canadian A Customer Nbr</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <type>Text</type>
    <unique>false</unique>
</CustomField>

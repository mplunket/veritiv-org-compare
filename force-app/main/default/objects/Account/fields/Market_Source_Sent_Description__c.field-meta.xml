<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Market_Source_Sent_Description__c</fullName>
    <description>This field will define all the accounts that were sent to Market Source, and how they were defined when we sent them.  Use the Market Source Flag to see what is included in the sync.  Inactive sent as prospect and prospect created from inactive are basically duplicates and we sync only the prospect.</description>
    <externalId>false</externalId>
    <label>Market Source Sent Description</label>
    <required>false</required>
    <trackFeedHistory>false</trackFeedHistory>
    <trackHistory>false</trackHistory>
    <type>Picklist</type>
    <valueSet>
        <restricted>true</restricted>
        <valueSetDefinition>
            <sorted>false</sorted>
            <value>
                <fullName>Active Sent to MS</fullName>
                <default>false</default>
                <label>Active Sent to MS</label>
            </value>
            <value>
                <fullName>Inactive Sent to MS as Prospect</fullName>
                <default>false</default>
                <label>Inactive Sent to MS as Prospect</label>
            </value>
            <value>
                <fullName>Prospect created from Inactive</fullName>
                <default>false</default>
                <label>Prospect created from Inactive</label>
            </value>
            <value>
                <fullName>Veritiv Prospect</fullName>
                <default>false</default>
                <label>Veritiv Prospect</label>
            </value>
            <value>
                <fullName>Prospect created from Marketing</fullName>
                <default>false</default>
                <isActive>false</isActive>
                <label>Prospect created from Marketing</label>
            </value>
        </valueSetDefinition>
    </valueSet>
</CustomField>

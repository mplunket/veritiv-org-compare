<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>End_Date_can_not_be_in_Past</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
Active__c = TRUE,
End_Date__c &lt; TODAY()
)</errorConditionFormula>
    <errorMessage>End Date can not be in the past on active Announcements</errorMessage>
</ValidationRule>

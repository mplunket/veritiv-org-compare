<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Mkt_Sales_Primary__c</fullName>
    <description>This formula field will display as TRUE if the Contact Role field has the value of Sales Primary Contact selected.</description>
    <externalId>false</externalId>
    <formula>IF(INCLUDES(Contact_Role__c,&quot;Sales Primary Contact&quot;),TRUE,FALSE)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Mkt Sales Primary?</label>
    <trackHistory>false</trackHistory>
    <type>Checkbox</type>
</CustomField>

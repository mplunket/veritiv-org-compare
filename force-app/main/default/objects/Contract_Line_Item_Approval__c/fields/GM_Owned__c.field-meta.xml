<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>GM_Owned__c</fullName>
    <description>Checks to see if a record is owned (assigned) to a GM or not. Records assigned to a GM should be editable and easy to find by the pricing team</description>
    <externalId>false</externalId>
    <formula>CONTAINS(Owner:User.Role_Name__c , &quot;- GM&quot;)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>Assigned to a GM?</inlineHelpText>
    <label>GM Owned?</label>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Checkbox</type>
</CustomField>

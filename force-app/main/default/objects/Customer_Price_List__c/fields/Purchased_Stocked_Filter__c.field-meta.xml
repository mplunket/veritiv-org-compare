<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Purchased_Stocked_Filter__c</fullName>
    <description>Looks at the Is_Stocked flag on the product or the 12 Month Sales Volume field on the contract line to determine whether the product should be shown or not.</description>
    <externalId>false</externalId>
    <inlineHelpText>Filter to show all agreement lines, just stocked products, or just products the customer has purchased in the past 12 months.</inlineHelpText>
    <label>Purchased/Stocked Filter</label>
    <required>true</required>
    <trackTrending>false</trackTrending>
    <type>Picklist</type>
    <valueSet>
        <restricted>true</restricted>
        <valueSetDefinition>
            <sorted>false</sorted>
            <value>
                <fullName>Show All Agreement Lines</fullName>
                <default>true</default>
                <label>Show All Agreement Lines</label>
            </value>
            <value>
                <fullName>Show Purchased Agreement Lines Only</fullName>
                <default>false</default>
                <label>Show Purchased Agreement Lines Only</label>
            </value>
            <value>
                <fullName>Show Stock Agreement Lines Only</fullName>
                <default>false</default>
                <label>Show Stock Agreement Lines Only</label>
            </value>
            <value>
                <fullName>Show Purchased Or Stocked Agreement Lines</fullName>
                <default>false</default>
                <label>Show Purchased Or Stocked Agreement Lines</label>
            </value>
        </valueSetDefinition>
    </valueSet>
</CustomField>

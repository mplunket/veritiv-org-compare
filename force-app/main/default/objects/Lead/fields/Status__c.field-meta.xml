<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Status__c</fullName>
    <description>Because the Lead Status used to stay as 2- Working even after conversion, use this field to truly distinquish active and converted.</description>
    <externalId>false</externalId>
    <formula>IF (IsConverted = True , &quot;Converted&quot;, TEXT(Status)  )</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Status</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <type>Text</type>
    <unique>false</unique>
</CustomField>

<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>X1_Day_to_1_Week_Value__c</fullName>
    <description>The number of Business Days (from 1 day to 1 week)</description>
    <externalId>false</externalId>
    <formula>if(and(Business_Days__c &gt; 1, Business_Week__c &lt;= 1),Business_Days__c,0)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>The number of Business Days, if the Opportunity Duration is between 1 Day and 1 Week.</inlineHelpText>
    <label>1 Day to 1 Week Value</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>

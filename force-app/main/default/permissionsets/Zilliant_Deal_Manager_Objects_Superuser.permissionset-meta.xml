        <modifyAllRecords>true</modifyAllRecords>
        <object>zpl__PriceLookup__c</object>
        <viewAllRecords>true</viewAllRecords>
    </objectPermissions>
    <userPermissions>
        <enabled>true</enabled>
        <name>LightningExperienceUser</name>
    </userPermissions>
</PermissionSet>
